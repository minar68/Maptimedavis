<body>
  <h2>D3firststeps</h2>

    <svg width="100" height="150">
        <rect x="25" width="15" fill="red"></rect>
        <rect x="50" width="15" fill="red"></rect>
        <rect x="75" width="15" fill="red"></rect>
      </svg>
  </div>
  <script src="https://d3js.org/d3.v5.min.js"></script>
<script>
  var population = [400, 600, 100];
d3.selectAll('rect')
  .data(population)
  .attr('height', function(d){
    return d/10 * 1.5;
  })
  .attr('y', function(d){
    return 150 - d/10 * 1.5;
  });
</script>
</body>
